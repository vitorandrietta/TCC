@model IList<Eletronics.WEB.Models.ClientScreenModel>
@{
    WebGrid gridConsulta = new WebGrid(Model, rowsPerPage: 10);
}

@gridConsulta.GetHtml(tableStyle: "webGrid",
      headerStyle: "header",
      alternatingRowStyle: "alt",
      selectedRowStyle: "select", columns:
      gridConsulta.Columns(gridConsulta.
      Column("FirstName", "Nome"),
      gridConsulta.Column("LastName", "Sobrenome"),
      gridConsulta.Column("CPF", "CPF"), gridConsulta.
      Column("RG", "RG"), gridConsulta.Column
      ("ClientTypeDescription", "Descrição do tipo de cliente")));

